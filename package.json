{
  "name": "frog-food-street-monorepo",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "workspaces": [
    "packages/*",
    "packages/effects/*",
    "apps/*",
    "internal/*"
  ],
  "scripts": {
    "build:a": "pnpm -r --filter web-admin run build",
    "build:s": "pnpm -r --filter mobile-supplier run build",
    "build:m": "pnpm -r --filter mobile-merchant run build",
    "dev": "pnpm -r --parallel run dev",
    "dev:a": "pnpm -r --filter web-admin run dev",
    "dev:s": "pnpm -r --filter mobile-supplier run dev",
    "dev:m": "pnpm -r --filter mobile-merchant run dev",
    "test": "pnpm -r run test",
    "lint": "pnpm dlx eslint .",
    "catalog": "pnpm dlx pact migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-frogfrog/frog-monorepo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/a-frogfrog/frog-monorepo/issues"
  },
  "homepage": "https://github.com/a-frogfrog/frog-monorepo#readme",
  "devDependencies": {
    "@frog/types": "workspace:*",
    "@tailwindcss/postcss": "catalog:style",
    "@types/js-cookie": "catalog:types",
    "@types/node": "catalog:types",
    "@types/nprogress": "catalog:types",
    "@vitejs/plugin-vue": "catalog:build",
    "@vue/tsconfig": "catalog:dev",
    "autoprefixer": "catalog:style",
    "echarts": "catalog:frontend",
    "eslint": "catalog:lint",
    "eslint-plugin-vue": "catalog:lint",
    "pncat": "catalog:cli",
    "postcss": "catalog:style",
    "prettier": "catalog:lint",
    "tailwindcss": "catalog:style",
    "turbo": "catalog:monorepo",
    "typescript": "catalog:dev",
    "vite": "catalog:build",
    "vite-plugin-dts": "catalog:build",
    "vite-plugin-inspect": "catalog:build",
    "vue-tsc": "catalog:dev"
  },
  "dependencies": {
    "@frog/vite-config": "workspace:*",
    "@tailwindcss/vite": "catalog:build",
    "@vue/shared": "catalog:prod",
    "axios": "catalog:network",
    "element-plus": "^2.10.4",
    "js-cookie": "catalog:prod",
    "nprogress": "catalog:frontend",
    "pinia": "catalog:frontend",
    "vant": "catalog:prod",
    "vue": "catalog:frontend",
    "vue-router": "catalog:frontend"
  }
}
